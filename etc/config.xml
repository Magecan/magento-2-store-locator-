<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <storelocator>
            <general>
                <enabled>0</enabled>
                <page_title>Store Locator</page_title>
                <map_height>600</map_height>
                <map_center_latitude>0</map_center_latitude>
                <map_center_longitude>0</map_center_longitude>
                <initial_zoom>0</initial_zoom>
                <radius_options>2,5,10,25</radius_options>
